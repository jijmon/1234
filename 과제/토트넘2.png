<script src="https://cdn.fourfourtwo.co.kr/script/share.sns.js"></script>
<script>
	var site_ismobile		= '';			// 현재 모바일접속 여부
	var kakaojavascriptkey	= '5cd2f3a9ec8272d77b2f9238127b8b4e';	// 카카오앱 자바스크립트키
	var articleIdxno		= '42640';
	var snsKakaoSend		= function(sns, url, title, desc, img){		// kakao용 함수
								if(sns == "kakao") {
									sendKakaoTalk(url, title, desc, img);
								}else if(sns == "kakaostory") {
									shareStory(url, title, desc, img);
								}
							};

	$(document).ready(function(e) {
		
		var sns_clipboard = new Clipboard('.sns_clipboard');
		sns_clipboard.on('success', function(e) {
			alert("클립보드에 복사되었습니다. \nCtrl+v해서 원하는 곳에 붙여넣으세요.");
		});

		$('button.sns-share-btns').on('click', function(e) {
			var sns		= $(this).data('sharesns');
			var title	= $(this).data('title');
			var desc	= $(this).data('desc');
			var url		= $(this).data('url');
			var img		= $(this).data('image');

			if(sns == "link") return true;

			// SNS 공유 통계 집계 (공유하기에 영향을 주지않도록 체크하지는 않음)
			snsShareStats(articleIdxno, sns);

			if($.inArray(sns, ["kakao","kakaostory"])>=0){
				// kakao가 이미 로딩 되었다면 실행 or not, url 로딩 시킴
				if("Kakao" in window) snsKakaoSend(sns, url, title, desc, img);
				else{
					var script=document.createElement("script");
					script.src="//developers.kakao.com/sdk/js/kakao.min.js";
					script.onload=function(e){
						if(kakaojavascriptkey){
							kakaoinit(kakaojavascriptkey);
							snsKakaoSend(sns, url, title, desc, img);
						}
					};
					script.onerror=function(e){};

					document.querySelector("head").appendChild(script);
				}

				return false;
			}

			if(sns == "band") {
				shareBand(url, title);
			}else if(sns == "telegram") {
                shareTelegram(url, title);
			}else{
				shareOtherSNS(sns, title, url, img);
			}

		});
	});
</script>
